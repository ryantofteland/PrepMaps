<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8">
        <title>@ViewBag.Title</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="">
        <meta name="author" content="Toftware Consulting">

		<!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
		<!--[if lt IE 9]>
			<script src="@Url.Content("~/Content/js/html5.js")"></script>
		<![endif]-->

		<link href="@Url.Content("~/Content/css/bootstrap.css")" rel="stylesheet">
		<link href="@Url.Content("~/Content/css/prepmaps.css")" rel="stylesheet">

		<link rel="shortcut icon" href="@Url.Content("~/Content/images/favicon.ico")">
		<link rel="apple-touch-icon" href="@Url.Content("~/Content/images/touch-icon.png")">

		<script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?sensor=true"></script>
		<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js"></script>
		<script type="text/javascript" src="@Url.Content("~/Content/js/underscore.js")"></script>
		<script type="text/javascript" src="@Url.Content("~/Content/js/mustache.js")"></script>
		<script type="text/javascript" src="@Url.Content("~/Content/js/backbone.js")"></script>    
		<script type="text/javascript" src="@Url.Content("~/Content/js/PrepMaps.js")"></script>
		<script type="text/javascript" src="@Url.Content("~/Content/js/PrepMaps.Backbone.js")"></script>

		@RenderSection("Head", false)
	</head>

	<body>
		<div class="navbar navbar-fixed-top">
			<div class="navbar-inner">
				<div class="container-fluid">
					<h1 class="brand">PrepMaps - Minnesota</h1>
				</div>
			</div>
		</div>

		<div class="container-fluid">
			@RenderBody()
		</div>

		@RenderSection("FooterScripts")
	</body>
</html>